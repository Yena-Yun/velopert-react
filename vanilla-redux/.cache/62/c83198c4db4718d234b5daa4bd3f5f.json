}},{"name":"increase","generated":{"line":113,"column":17},"source":"index.js","original":{"line":95,"column":19}},{"generated":{"line":113,"column":25},"source":"index.js","original":{"line":95,"column":27}},{"generated":{"line":113,"column":26},"source":"index.js","original":{"line":95,"column":28}},{"generated":{"line":113,"column":27},"source":"index.js","original":{"line":95,"column":27}},{"generated":{"line":113,"column":28},"source":"index.js","original":{"line":95,"column":4}},{"generated":{"line":114,"column":0},"source":"index.js","original":{"line":96,"column":1}},{"generated":{"line":114,"column":1},"source":"index.js","original":{"line":94,"column":0}},{"name":"btnDecrease","generated":{"line":116,"column":0},"source":"index.js","original":{"line":97,"column":0}},{"generated":{"line":116,"column":11},"source":"index.js","original":{"line":97,"column":11}},{"name":"onclick","generated":{"line":116,"column":12},"source":"index.js","original":{"line":97,"column":12}},{"generated":{"line":116,"column":19},"source":"index.js","original":{"line":97,"column":0}},{"generated":{"line":116,"column":22},"source":"index.js","original":{"line":97,"column":22}},{"generated":{"line":116,"column":34},"source":"index.js","original":{"line":97,"column":28}},{"name":"store","generated":{"line":117,"column":0},"source":"index.js","original":{"line":98,"column":4}},{"name":"store","generated":{"line":117,"column":2},"source":"index.js","original":{"line":98,"column":4}},{"generated":{"line":117,"column":7},"source":"index.js","original":{"line":98,"column":9}},{"name":"dispatch","generated":{"line":117,"column":8},"source":"index.js","original":{"line":98,"column":10}},{"generated":{"line":117,"column":16},"source":"index.js","original":{"line":98,"column":4}},{"name":"decrease","generated":{"line":117,"column":17},"source":"index.js","original":{"line":98,"column":19}},{"generated":{"line":117,"column":25},"source":"index.js","original":{"line":98,"column":27}},{"generated":{"line":117,"column":27},"source":"index.js","original":{"line":98,"column":4}},{"generated":{"line":118,"column":0},"source":"index.js","original":{"line":99,"column":1}},{"generated":{"line":118,"column":1},"source":"index.js","original":{"line":97,"column":0}}],"sources":{"index.js":"import { createStore } from 'redux';\r\n\r\n//Dom 레퍼런스 (vanilla JS로 HTML에 있는 DOM 요소 선택)\r\nconst divToggle = document.querySelector('.toggle');\r\nconst counter = document.querySelector('h1');\r\nconst btnIncrease = document.querySelector('#increase');\r\nconst btnDecrease = document.querySelector('#decrease');\r\n\r\n//액션\r\nconst TOGGLE_SWITCH = 'TOGGLE_SWITCH';\r\nconst INCREASE = 'INCREASE';\r\nconst DECREASE = 'DECREASE';\r\n\r\n//액션생성함수\r\nconst toggleSwitch = () => ({ type: TOGGLE_SWITCH });\r\nconst increase = (difference) => ({ type: INCREASE, difference });\r\nconst decrease = () => ({ type: DECREASE });\r\n\r\n//초기값(초기state)\r\nconst initialState = {\r\n    toggle: false,\r\n    counter: 0\r\n}\r\n\r\n//reducer 함수 : 변화를 일으키는 함수\r\n//action의 type에 따라 state를 변화시킨 결과물 반환\r\n//(state가 undefined일 때는 initialState를 기본값으로 사용)\r\nfunction reducer(state = initialState, action) {\r\n    //action.type에 따라 다른 작업을 처리함\r\n    switch (action.type) {\r\n        case TOGGLE_SWITCH:\r\n            return {\r\n                //기존 state(initialState)\r\n                ...state,\r\n                //전달받은 액션객체의 특정 state 변화시킴\r\n                //여기서는 toggle state가 true인 경우는 false로, false인 경우는 true로 변화시킴\r\n                toggle: !state.toggle,\r\n            };\r\n        case INCREASE:\r\n            return {\r\n              ...state,\r\n              // 기존 counter state에 액션에서 전달받은 difference를 추가하여 변화시킴\r\n              counter: state.counter + action.difference,\r\n            };\r\n        case DECREASE:\r\n            return {\r\n                ...state,\r\n                counter: state.counter - 1\r\n            };\r\n        default:\r\n            return state;\r\n    }\r\n}\r\n\r\n//reducer에서 처리한 내용을 store를 생성해서 저장\r\nconst store = createStore(reducer);\r\n\r\n//render 함수 => state가 업데이트될 때마다 호출됨\r\nconst render = () => {\r\n    //store에 저장되어 있는 현재 상태를 불러온다.\r\n    const state = store.getState();\r\n\r\n    //토글 처리\r\n    //toggle이 true인 경우에는 \r\n    //divToggle의 class에 'active'를 추가하고\r\n    //(divToggle이 active인 경우에는 노란색으로 변함)\r\n    if (state.toggle) {\r\n        divToggle.classList.add('active');\r\n    //toggle이 false인 경우에는\r\n    //divToggle의 class에서 'active'를 제거\r\n    } else {\r\n        divToggle.classList.remove('active');\r\n    }\r\n\r\n    //카운터 처리\r\n    //counter DOM의 안쪽 text를 state의 counter로 바꿈\r\n    counter.innerText = state.counter;\r\n};\r\n\r\n//render 함수: 상태가 '업데이트'될 때마다 호출됨\r\nrender();\r\n\r\n//store에 render함수를 구독\r\n//subscribe: 파라미터로 전달된 함수를 추후 액션이 발생하여 상태가 업데이트될 때마다 호출\r\n//store의 상태가 바뀔 때마다 render함수가 호출되도록 함\r\nstore.subscribe(render);\r\n\r\n//dispatch(액션생성함수에서 만든 액션 객체) : 액션을 발생시킴\r\n//store.dispatch(액션 객체) : 발생시킨 액션을 store에게 전달\r\n//각 DOM을 클릭 시 store에 액션함수를 전달하게 함\r\ndivToggle.onclick = () => {\r\n    store.dispatch(toggleSwitch());\r\n};\r\nbtnIncrease.onclick = () => {\r\n    store.dispatch(increase(1));\r\n}\r\nbtnDecrease.onclick = () => {\r\n    store.dispatch(decrease());\r\n}"},"lineCount":null}},"error":null,"hash":"287293ed69ff551f414acfe6002fe1c4","cacheData":{"env":{}}}